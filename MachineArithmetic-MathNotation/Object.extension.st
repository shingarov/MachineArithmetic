Extension { #name : #Object }

{ #category : #'*MachineArithmetic-MathNotation' }
Object >> ∈ [ aCollection
	^aCollection includes: self
]

{ #category : #'*MachineArithmetic-MathNotation' }
Object >> ∘ [ f
	^f value: self
]

{ #category : #'*MachineArithmetic-MathNotation' }
Object >> ≅ [ anObject
	^self = anObject
]

{ #category : #'*MachineArithmetic-MathNotation' }
Object >> ⊛ [ Fa
	^Fa collect: self
]
