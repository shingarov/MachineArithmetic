Class {
	#name : #Real,
	#superclass : #Z3ArithmeticNode,
	#category : #'Z3-Core'
}

{ #category : #types }
Real class >> sort [
	^ Z3Sort real
]

{ #category : #testing }
Real >> isReal [
	^true
]

{ #category : #adapting }
Real >> retry: selector coercing: rhs [
	^rhs adaptToReal: self andSend: selector
]

{ #category : #converting }
Real >> toInt [
	^ Z3 mk_real2int: ctx _: self

]

{ #category : #converting }
Real >> toReal [
	^ self

]
