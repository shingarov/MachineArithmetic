{ #category : #accessing }
TBool class >> instance [ 
	instance isNil ifTrue: [ instance := self basicNew ].
	^instance
]

{ #category : #'as yet unclassified' }
TBool >> baseSort [
	^Bool sort
]

{ #category : #printing }
TBool >> printOn: aStream [
	aStream nextPutAll: 'TBool'
]
